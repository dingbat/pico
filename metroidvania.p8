-- physics

function collide(p,ax)
	x = p.x[ax]  -- position
	v = p.v[ax]  -- velocity
	
	-- size of bnding box along ax
	bbw = p.bb[2*ax]-p.bb[2*ax-1]
	
	dx = abs(v)%bbw*sgn(v)
	onflr = false
	repeat
	 x1 = x+dx
	 pt = {p.x[1],p.x[2]}
	 pt[ax] = x1
	 if bbhit(pt,p.bb) then
	 	-- check if we hit the ground
			gd = gdir(p)[ax]
			onflr = sgn(v)==gd
			if onflr then
			 p.jmpt = max(p.jmpt-1,0)
		 end
		 
	  -- snap to surface
	  x1 = flr(x1+(-sgn(v)+1)/2)
	 
	  -- bounce a little
	  p.v[ax] = -p.v[ax]*p.bnce[ax]

	  break
	 else
	  -- fixme don't overshoot
	  dx += sgn(v)*bbw*0.5
	 end
	until abs(dx)>abs(v)
	p.x[ax] = x1
	
	return onflr
end

function flipg(p,ax,sn)
 oax = (ax%2)+1
 p.a[oax] = 0.1*p.a[oax]
 
 -- if gravity axis changed 
 -- since last frame, then
 -- rotate everything
 if p.gax!=ax then
   --p.a[oax] = -p.a[oax]
   p.bb = {p.bb[3],p.bb[4],
           p.bb[1],p.bb[2]}
   p.frct = {p.frct[2],
             p.frct[1]}
   p.bnce = {p.bnce[2],
             p.bnce[1]}
 end
 
 if sn==-1 then
  p.a[ax] = max(p.a[ax]-dg,-g)
 elseif sn==1 then
  p.a[ax] = min(p.a[ax]+dg,g)
 else
  assert(false)
 end
 
 p.gax = ax
 p.gsn = sn
end

function move_phys_obs(p)
 onflrx = collide(p,1)
	onflry = collide(p,2)
	p.on_flr = onflrx or onflry
	
	-- re-orient gravity
	if bbhit(p.x,p.bb,2) then
	 flipg(p,2,-1)
	elseif bbhit(p.x,p.bb,3) then
	 flipg(p,1,-1)
	elseif bbhit(p.x,p.bb,4) then
	 flipg(p,1,1)
	elseif bbhit(p.x,p.bb,5) then
	 flipg(p,2,1)
	end
	
	-- gravity and air resistance
	p.v[1] += p.a[1]
	p.v[2] += p.a[2]
	p.v[1] *= p.frct[1]
	p.v[2] *= p.frct[2]

	-- we should never end a frame
	-- intersecting a solid object
	-- assert(not bbhit(p.x,p.bb))
end

