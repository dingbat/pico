-- utility functions

-- test if point is solid
function pt_hit(x, y, flag)	
	if (x < 0 or x >= 128) then
	  return true
	end
	spr_id = mget(x, y)
	return fget(spr_id, flag or 0)
end

-- test if box collides with 
-- environment (true if tile
-- matches flag)
function bbhit(pt, bb, flag)
 x = pt[1]
 y = pt[2]
	pts = {{x+bb[1], y+bb[3]},
	       {x+bb[1], y+bb[4]},
	       {x+bb[2], y+bb[4]},
	       {x+bb[2], y+bb[3]}}
	for p in all(pts) do
		if pt_hit(p[1],p[2],flag) then
			return true
		end
	end
	
	return false
end


-- direction vector of gravity
function gdir(p)
 assert(p.gsn==1 or p.gsn==-1)
	if p.gax == 1 then
	 return {sgn(p.gsn),0}
	elseif p.gax == 2 then
	 return {0,sgn(p.gsn)} 
	else
	 assert(false)
	end
end

-- log to screen
function dbg(str)
	dbg_txt = dbg_txt..str.."\n"
end

-- clear log (called once per
-- frame)
function dbg_clr()
 dbg_txt = ""
end

